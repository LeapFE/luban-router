{
  "name": "luban-router",
  "version": "0.0.1",
  "description": "a configurable routing solution for react app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "scripts": {
    "test": "npm run test",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "check-type": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "clean": "rm -rf dist",
    "pre:release": "npm run clean && npm run format && npm run eslint",
    "build": "npm run clean && cross-env NODE_ENV=production tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/front-end-captain/luban-router.git"
  },
  "keywords": [
    "router",
    "react",
    "configurabl"
  ],
  "author": "front-end-captain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/front-end-captain/luban-router/issues"
  },
  "homepage": "https://github.com/front-end-captain/luban-router#readme",
  "devDependencies": {
    "@types/react": "^16.9.17",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-router-dom": ">=5.0.0"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run eslint",
      "npm run format",
      "git add"
    ]
  }
}
