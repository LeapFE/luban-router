{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:react/recommended", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended", "airbnb"],
  "plugins": ["@typescript-eslint", "react"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "prama": "React",
      "version": "detect"
    }
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "tsx": true
    }
  },
  "rules": {
    "quotes": ["error", "double"],
    "import/no-unresolved": ["off"],
    "react/jsx-filename-extension": ["error", { "extensions": [".tsx"] }],
    "react/prop-types": "off",
    "import/extensions": "off"
  }
}
